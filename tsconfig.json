{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],

      "@content/*": ["./src/content/*"],

      "@contracts/*": ["./src/contracts/*"],
      "@contracts": ["./src/contracts"],

      "@icons/*": ["./src/components/icons/*"],
      "@icons": ["./src/components/icons"],

      "@layouts/*": ["./src/layouts/*"]
    }
  }
}
