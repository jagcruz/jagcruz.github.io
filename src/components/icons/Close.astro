---
interface Props {
  width?: number;
  height?: number;
  strokeWidth?: number;
}

const { width = 24, height = 24, strokeWidth = 2 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  stroke="currentColor"
  stroke-linecap="round"
  stroke-linejoin="round"
  stroke-width={strokeWidth}
>
  <title>Close</title>
  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
  <path d="M18 6l-12 12"></path>
  <path d="M6 6l12 12"></path>
</svg>
